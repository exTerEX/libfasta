{
    "name": "",
    "image": "exterex/cpp-dev",
    "updateRemoteUserUID": false,
    "runArgs": [
        "-v",
        "/etc/localtime:/etc/localtime:ro"
    ],
    "extensions": [
        "ms-vscode.cpptools",
        "twxs.cmake",
        "ms-vscode.cmake-tools",
        "docsmsft.docs-markdown",
        "xaver.clang-format",
        "cschlosser.doxdocgen",
        "cheshirekow.cmake-format",
        "notskm.clang-tidy"
    ],
    "postCreateCommand": "make install"
}
